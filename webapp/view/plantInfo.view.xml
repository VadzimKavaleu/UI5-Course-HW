<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="ZKAV.HW2_UI5_COURSE.controller.plantInfo">
	<App>
		<pages>
			<Page title="{i18n>modelViewInfo}">
				<headerContent>
					<Button xmlns="sap.m" xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" id="button0"
						icon="sap-icon://navigation-left-arrow" press="action" action:wiring="\{'press':\{'navigation':\{'routeName':'View3'\}\}\}"/>
				</headerContent>
				<content>
					<Panel id="panel1" headerText="{i18n>panelHeader}" class="sapUiResponsiveMargin" width="auto">
						<headerToolbar>
							<Toolbar>
								<Title text="{i18n>person} {person>Num1}" level="H2"/>
								<ToolbarSpacer/>
							</Toolbar>
						</headerToolbar>
						<content>
							<Label text="{i18n>personId}" width="100%" id="label0"/>
							<Input xmlns="sap.m" value="{person>Num1}" id="input0" width="20%"/>
							<Label text="{i18n>personName}" width="100%" id="label1"/>
							<Input xmlns="sap.m" value="{person>Name1}" id="input1" width="40%"/>
							<Label text="{i18n>personDescription}" width="100%" id="label2"/>
							<Input xmlns="sap.m" value="{person>Description1}" id="input2" width="40%"/>
							<Label text="{i18n>personCourse}" width="100%" id="label3"/>
							<Input xmlns="sap.m" value="{person>Course1}" id="input3" width="20%"/>
							<Label text="{i18n>personGroup}" width="100%" id="label4"/>
							<Input xmlns="sap.m" value="{person>Group1}" id="input4" width="20%"/>
						</content>
					</Panel>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>